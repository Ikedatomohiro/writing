# OpenAI API Key
OPENAI_API_KEY=sk-xxx

# SerpAPI Key (optional - モック結果を使用する場合は不要)
SERPAPI_API_KEY=xxx

# Google OAuth 2.0 (管理画面ログイン用)
# Google Cloud ConsoleでOAuth 2.0クライアントIDを作成して設定
GOOGLE_CLIENT_ID=xxx
GOOGLE_CLIENT_SECRET=xxx

# Auth.js Secret (openssl rand -base64 32 で生成)
AUTH_SECRET=xxx

# ホワイトリスト (カンマ区切りで複数指定可能)
ALLOWED_EMAILS=user@example.com
